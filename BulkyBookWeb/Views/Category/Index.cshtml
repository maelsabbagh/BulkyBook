@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Index Category</h1>

<table class="table table-bordered table-striped" style="width:100%" >
    <thead>
        <tr>
            <th>Category Name</th>
            <th>Dispaly Order</th>
        </tr>
            
    </thead>
    <tbody>
        <tr>
            @foreach(var obj in Model)
            {
                <td width="50%">
                    @obj.Name
                </td>
                <td width="30%">
                    @obj.DisplayOrder
                </td>
            }
        </tr>
    </tbody>

</table>

